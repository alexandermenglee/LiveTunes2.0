@model IEnumerable<LiveTunes.MVC.Models.Survey>

@{
    ViewData["Title"] = "Index";
}

<h4>My Survey</h4>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ArtistName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FavoriteGenre1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FavoriteGenre2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FavoriteGenre3)
            </th>
            <th>
                Action
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ArtistName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FavoriteGenre1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FavoriteGenre2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FavoriteGenre3)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
}
    </tbody>
</table>
